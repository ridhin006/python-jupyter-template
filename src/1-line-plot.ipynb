    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "x=np.arange(0,11,)\n",
    "y=x**2\n",
    "\n",
    "plt.plot(x,y,color='r',marker='o')\n",
    "plt.title(\"Line plot\")\n",
    "plt.xlabel('X axis')\n",
    "plt.ylabel('Y axis')\n",
    "plt.show()"
